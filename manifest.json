{
    "manifest_version": 2,
    "name": "poseidon-proxy",
    "version": "2.0.1",
    "description": "https://github.com/Poseidon-Service-Framework/poseidon-chrome-proxy",
    "icons": {
        "16": "factory.png",
        "48": "factory.png",
        "128": "factory.png"
    },
    "browser_action": {
        "default_title": "poseidon-proxy",
        "default_popup": "./popup.html"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "proxy",
        "storage",
        "cookies",
        "<all_urls>"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}
